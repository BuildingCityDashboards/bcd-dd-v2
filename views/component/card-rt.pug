mixin card-rt(href, style, Header, title, Chartid)
  a(href=href class="card__link")
    div(class=style)
      .card__header=Header
      .card__body.card-graph
        div(id=Chartid width="205" height="70")
           .container-fluid
                .row
                    #card-left.col-sm-4
                        
                    #card-center.col-sm-4
                        
                    #card-right.col-sm-4
                        
                    